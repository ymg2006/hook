{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug hook Extension",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "../../x64/Debug_TS/php.exe",
            "args": [
                "-d extension_dir=../Debug_TS",
                "-d extension=php_hook.dll",
                "../../ext/hook/test.php"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}
